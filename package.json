{
  "name": "auto-download-image",
  "version": "1.0.2",
  "description": "Automatically detect and download online images embedded in notes to your local device, with support for customizing the save location, filename format, and path type.",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "dev": "node esbuild.config.mjs",
    "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
    "version": "node version-bump.mjs && git add manifest.json versions.json",
    "lint": "eslint ."
  },
  "keywords": [
    "obsidian",
    "plugin",
    "image",
    "download",
    "network",
    "offline",
    "markdown",
    "auto"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.11.30",
    "esbuild": "^0.25.5",
    "globals": "^14.0.0",
    "tslib": "^2.8.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.35.1",
    "@eslint/js": "^9.30.1",
    "jiti": "^2.6.1"
  },
  "dependencies": {
    "obsidian": "^1.7.5"
  }
}
